<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBranco"
    tools:context=".atividade.SplashLogo">

    <ImageView
        android:id="@+id/imageProjeto"
        android:layout_width="@dimen/width_heigth_image_PROJETO"
        android:layout_height="@dimen/width_heigth_image_PROJETO"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/copy" />


    <ImageView
        android:id="@+id/imageIfce"
        android:layout_width="@dimen/width_heigth_image_IFCE"
        android:layout_height="@dimen/width_heigth_image_IFCE"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo" />

</RelativeLayout>